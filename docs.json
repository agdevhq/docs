{
    "$schema": "https://mintlify.com/docs.json",
    "theme": "maple",
    "name": "Ag.dev",
    "logo": {
        "light": "/public/logo.svg",
        "dark": "/public/logo-inverse.svg"
    },
    "favicon": "/public/favicon.ico",
    "colors": {
        "primary": "#000000",
        "light": "#ffffff",
        "dark": "#000000"
    },
    "icons": {
        "library": "lucide"
    },
    "navigation": {
        "dropdowns": [
            {
                "dropdown": "Documentation",
                "description": "Learn how to use Ag.dev",
                "icon": "book",
                "iconType": "regular",
                "groups": [
                    {
                        "group": "Getting started",
                        "pages": ["index", "quickstart"]
                    },
                    {
                        "group": "Core Concepts",
                        "pages": [
                            "concepts/agents",
                            "concepts/versioning",
                            "concepts/runs",
                            "concepts/tools",
                            "concepts/inputs"
                        ]
                    }
                ]
            },
            {
                "dropdown": "API Reference",
                "description": "Using the Ag.dev API",
                "icon": "terminal",
                "openapi": "openapi.json"
            }
        ],
        "global": {
            "anchors": [
                {
                    "anchor": "Discord Community",
                    "href": "https://discord.gg/cPSJbuKDse",
                    "icon": "discord"
                }
            ]
        }
    },
    "navbar": {
        "links": [
            {
                "label": "Support",
                "href": "mailto:hi@ag.dev"
            }
        ],
        "primary": {
            "type": "button",
            "label": "Console",
            "href": "https://console.ag.dev"
        }
    },
    "contextual": {
        "options": ["copy", "view"]
    },
    "footer": {
        "socials": {
            "discord": "https://discord.gg/cPSJbuKDse"
        }
    }
